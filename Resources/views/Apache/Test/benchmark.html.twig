{% extends 'BenchmarkBundle:Layout:base.html.twig' %}

{% block header %}
{% include 'BenchmarkBundle:Header:header.html.twig' %}
{% endblock %}
{% block bodyContent %}
<div class="page-title">
	<div class="title_left">
		<h3>Benchmark</h3>
	</div>
</div>
<div class="clearfix"></div>
<div class="row">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<div class="x_panel">
			<div class="x_title">
				<h2>Form Test<small>Lanzar Prueba</small></h2>
				<div class="clearfix"></div>
			</div>
			<div class="x_content">
				<br>

				{{ form_start(
					form,
					{
						'attr'   : {'class': 'form-horizontal form-label-left'}
					}
				)
				}}
    				 {{ form_errors(form) }}

					<div class="form-group">
						 {{ form_label(form.Programa,null, {'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}}) }}
						<div class="col-md-9 col-sm-9 col-xs-12">
					        {{ form_widget(form.Programa,{'attr': {'class': 'select2_single form-control'}}) }}
							{{ form_errors(form.Programa) }}
						</div>
					</div>
					<div class="form-group">
						 {{ form_label(form.Fichero,null, {'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}}) }}
						<div class="col-md-9 col-sm-9 col-xs-12">
					        {{ form_widget(form.Fichero,{'attr': {'class': 'select2_single form-control'}}) }}
							{{ form_errors(form.Fichero) }}
						</div>
					</div>
					<div class="form-group">
						 {{ form_label(form.Algoritmo,null, {'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}}) }}
						<div class="col-md-9 col-sm-9 col-xs-12">
									{{ form_widget(form.Algoritmo,{'attr': {'class': 'select2_single form-control'}}) }}
							{{ form_errors(form.Algoritmo) }}
						</div>
					</div>
					<div class="form-group">
						 {{ form_label(form.Conf_del_Programa,null, {'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}}) }}
						<div class="col-md-9 col-sm-9 col-xs-12">
									{{ form_widget(form.Conf_del_Programa,{'attr': {'class': 'select2_single form-control'}}) }}
							{{ form_errors(form.Conf_del_Programa) }}
						</div>
					</div>
					<div class="form-group">
						 {{ form_label(form.Conf_del_Algoritmo,null, {'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}}) }}
						<div class="col-md-9 col-sm-9 col-xs-12">
									{{ form_widget(form.Conf_del_Algoritmo,{'attr': {'class': 'select2_single form-control'}}) }}
							{{ form_errors(form.Conf_del_Algoritmo) }}
						</div>
					</div>
					<div class="ln_solid"></div>
					<div class="form-group">
						<div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
							{{ form_widget(form.Empezar,{'attr': {'class': 'btn btn-success'}}) }}
						</div>
					</div>

				{{ form_end(form) }}
			</div>
		</div>
	</div>
<div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <h2>Paramentos especiales de conf.<small></small></h2>
                                <ul class="nav navbar-right panel_toolbox">
                                    <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                    </li>
                                    <li class="dropdown">
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                    </li>
                                    <li><a class="close-link"><i class="fa fa-close"></i></a>
                                    </li>
                                </ul>
                                <div class="clearfix"></div>
                            </div>
                            <div class="x_content">

                                <div class="">
                                    <ul class="to_do">
                                        <li>
                                            <p>
                                                <div class="icheckbox_flat-green" style="position: relative;"><input type="checkbox" class="flat" style="position: absolute; opacity: 0;"><ins class="iCheck-helper" style="position: absolute; top: 0%; left: 0%; display: block; width: 100%; height: 100%; margin: 0px; padding: 0px; border: 0px; opacity: 0; background: rgb(255, 255, 255);"></ins></div> Espacio en blanco : blankspace </p>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
</div>


{% endblock %}
