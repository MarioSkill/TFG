<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BenchmarkBundle\Entity\Ficheros" table="Ficheros">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ruta" type="string" column="ruta" length="125" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="size" type="string" column="size" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ext" type="string" column="ext" length="4" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="rutadatadocker" type="string" column="rutaDataDocker" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
